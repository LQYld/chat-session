    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-less": "^1.0.6",
    "stylelint-order": "^6.0.2",
    "tailwindcss": "^3.2.7",
    "url-loader": "^4.1.1",
    "webpack-obj-loader": "^1.0.4"
